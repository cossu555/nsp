<?xml version="1.0" encoding="UTF-8"?>
<module type="PYTHON_MODULE" version="4">
  <!-- Root element specifying the module configuration for a Python module, with version 4 schema. -->
  <component name="NewModuleRootManager">
    <!-- The component managing the root configuration of the module. -->
    <content url="file://$MODULE_DIR$">
      <!-- Defines the content URL of the module's directory. 
           - $MODULE_DIR$ is a placeholder for the actual directory of the module. -->
      <excludeFolder url="file://$MODULE_DIR$/.venv" />
      <!-- Excludes the `.venv` directory from the module content. 
           This is commonly used to prevent the virtual environment folder from being indexed or included in project operations. -->
    </content>
    <orderEntry type="inheritedJdk" />
    <!-- Specifies that the module uses the JDK inherited from the project configuration. 
         This ensures that the module will use the default JDK version set for the project. -->
    <orderEntry type="sourceFolder" forTests="false" />
    <!-- Specifies that the module contains a source folder, which holds the source code for the module.
         - `forTests="false"` indicates that this source folder is not for test code. -->
  </component>
</module>
